client
dev tun
proto udp
remote SERVER_HERE 443
resolv-retry infinite
nobind
persist-key
persist-tun
persist-remote-ip
ca ./etc/ca.ipvanish.com.crt
remote-cert-tls server
auth-nocache
auth-user-pass ./etc/login.config
comp-lzo
verb 3
auth SHA256
cipher AES-256-CBC
keysize 256
tls-cipher TLS-DHE-RSA-WITH-AES-256-CBC-SHA
