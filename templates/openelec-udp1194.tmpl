client
dev tun
proto udp
remote SERVER_HERE 1194
resolv-retry infinite
nobind
persist-key
persist-tun
persist-remote-ip
keepalive 10 120
ca CERT_HERE
remote-cert-tls server
#auth-nocache removed as user-pass is not re-read on a reconnect
#auth-nocache
auth-user-pass LOGIN_HERE
comp-lzo
verb 3
auth SHA256
cipher AES-256-CBC
keysize 256
tls-cipher TLS-DHE-RSA-WITH-AES-128-CBC-SHA
